{
  "manifest_version": 2,
  "name": "Dark-Meet",
  "description": "A dark theme for Google Meet.",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/"
      ],
      "css": ["theme/index.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "pages/popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
